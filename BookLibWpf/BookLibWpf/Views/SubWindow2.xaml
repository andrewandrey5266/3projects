<Window x:Class="BookLibWpf.Views.SubWindow2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SubWindow2" Height="116.418" Width="300">
    
        <StackPanel>
            <TextBlock Name="FeatureText" Margin="5" Text="Name" FontWeight="Bold" FontFamily="Cambria" />
            <TextBox MinWidth="150" HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="FeatureTextBox"/>

            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button x:Name="Yes1Button" Margin="5" Content="Submit" Background="{x:Null}" Click="YesButton_Click"/>
                <Button x:Name="No1Button" Margin="5" Content="Cancel" Background="{x:Null}" Click="NoButton_Click" />
            </StackPanel>
        </StackPanel>
       
</Window>
